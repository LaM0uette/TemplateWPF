<UserControl x:Class="TemplateWPF.Views.DemoViews"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:demoViews="clr-namespace:TemplateWPF.Views.Demo"
             mc:Ignorable="d">
    <Grid>
        
        <StackPanel Background="SlateGray">
            <Label Style="{StaticResource LabelDemo}" Content="Buttons"/>
            <demoViews:ButtonView/>
            
            <Label Style="{StaticResource LabelDemo}" Content="CheckBox"/>
            <demoViews:CheckBoxView/>
        </StackPanel>
        
    </Grid>
</UserControl>
